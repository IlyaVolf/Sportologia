        <attr name="contentTabsText3" format="string" />
        <attr name="contentTabsText4" format="string" />

        <attr name="contentTabsTextSizeSp" format="string"/>

        <attr name="contentTabsButtonsNumber" format="integer"/>

        <attr name="contentTabsActiveByDefault" format="integer"/>

        <attr name="contentTabsSpinnerSupport" format="boolean" />
        <attr name="contentTabsCounterSupport" format="boolean" />

        <attr name="photo_2" format="reference" />
        <attr name="photo_3" format="reference" />
        <attr name="photo_4" format="reference" />
        <attr name="photo_5" format="reference" />
        <attr name="photo_6" format="reference" />
        <attr name="photo_7" format="reference" />
        <attr name="photo_8" format="reference" />
        <attr name="photo_9" format="reference" />
        <attr name="photo_10" format="reference" />
